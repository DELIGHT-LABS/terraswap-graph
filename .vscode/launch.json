{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Dashboard",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "dashboard",
                "--watch"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "envFile": "${workspaceFolder}/.env",
            "type": "pwa-node"
        },
        {
            "name": "Server",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "start"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "envFile": "${workspaceFolder}/.env",
            "type": "pwa-node"
        },
        {
            "name": "Launch Collector",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "collect"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "envFile": "${workspaceFolder}/.env.local",
            "type": "pwa-node"
        },
        {
            "name": "Launch Bombay Collector",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "collect"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "TZ": "UTC",                         
                "TERRA_LCD": "https://bombay-lcd.terra.dev",
                "TERRA_CHAIN_ID": "bombay-12",
                "TERRA_MANTLE": "https://bombay-mantle.terra.dev",
                "START_BLOCK_HEIGHT": "5900001",
            },
            "type": "pwa-node"
        },
        {
            "name": "Launch Migrate",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "migrate"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "envFile": "${workspaceFolder}/.env",
            "type": "pwa-node"
        },
    ]
}